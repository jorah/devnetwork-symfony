{% extends 'base.html.twig' %}

{% block body %}
    <h1>Commentcode</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commentCode.id }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ commentCode.content }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if commentCode.createdAt %}{{ commentCode.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if commentCode.updatedAt %}{{ commentCode.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('commentcode_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('commentcode_edit', { 'id': commentCode.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
